<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="DailyFocus.View.LazyView.NewDailyView"
             xmlns:viewmdoel="clr-namespace:DailyFocus.ViewModel"
             xmlns:lazycontent="clr-namespace:DailyFocus.View"
             xmlns:lazyview="clr-namespace:DailyFocus.Resources.Utilities"
             Shell.TabBarIsVisible="False"
             Shell.BackgroundColor="{DynamicResource BackGround}"
             BackgroundColor="{DynamicResource BackGround}">

    <ContentPage.BindingContext>
        <viewmdoel:NewDailyVM/>
    </ContentPage.BindingContext>

    <Grid VerticalOptions="FillAndExpand" HorizontalOptions="FillAndExpand">
        <lazyview:LazyView x:Name="lazy" x:TypeArguments="lazycontent:NewDaily" BindingContext="{viewmdoel:NewDailyVM}"/>
    </Grid>
</ContentPage>